{
    "name": "ConsultasDriveapi",
    "version": "1.0",
    "description": "usar api de drive para distintas consultas",
    "manifest_version": 3,
    "action": {
      "default_title": "probar consultas con un boton",
      "default_popup": "index.html"  
    },
    "background": {
      "service_worker": "background.js"
    },
    "key": "",
    "oauth2": {
       "client_id": "758095101585-u69sctgsf0l316i2ui7e2qkp3pj9j5b9.apps.googleusercontent.com",
       "scopes":["https://www.googleapis.com/auth/drive.metadata.readonly"]
    },
    "permissions": [
        "identity","activeTab","scripting","tabs"
    ],
    "content_scripts":[
      {
       "matches": [
          "https://meet.google.com/*"
        ],
      "js": ["content.js"]
      }
    ]
    
 }